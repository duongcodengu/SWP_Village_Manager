@model List<Village_Manager.Models.CartItem>
@{
    ViewData["Title"] = "Success";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    var address = ViewBag.Address as Village_Manager.Models.Address;
    var orderId = ViewBag.OrderId?.ToString() ?? "";
    var paymentMethod = ViewBag.PaymentMethod?.ToString() ?? "N/A";
    var total = Model.Sum(i => (i.Product?.Price ?? 0) * i.Quantity);
    var discount = ViewBag.DiscountAmount != null ? (int)ViewBag.DiscountAmount : 0;
    var shipping = 20000;
    var finalTotal = total - discount + shipping;
    var itemCount = Model.Sum(i => i.Quantity);

}
